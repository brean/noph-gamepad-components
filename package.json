{
	"name": "noph-gamepad-components",
	"version": "0.1.0",
	"license": "BSD-3-Clause",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"package": "npm run prepack",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"overrides": {
		"cookie": "^1.0.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@materialx/material-color-utilities": "^0.4.8",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.4",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"noph-ui": "^0.31.5",
		"publint": "^0.3.16",
		"svelte": "^5.46.3",
		"svelte-check": "^4.3.5",
		"svelte-sequential-preprocessor": "^2.0.3",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"keywords": [
		"control",
		"svelte",
		"material",
		"material 3",
		"gamepad",
		"m3",
		"ui",
		"frontend",
		"ui-library"
	],
	"dependencies": {
		"svelte-gamepad-virtual-joystick": "^1.0.15"
	}
}
